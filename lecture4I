#include <iostream>
using namespace std;

int main(){
    int a , b;
    int c , d;
    cout << "Input 4 integer a, b, c and d" << endl;
    cin >> a;
    cin >> b;
    cin >> c;
    cin >> d;

    //statement (a>b)

    cout << (a>b && c>d); // and condition
    cout << (a>b || c>d); // or condition
}
